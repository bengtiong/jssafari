{{!-- Warning --}}
<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">London, UK</div>
        <div class="panel-body">
          <table class="table table-hover">
            <thead>
              <tr>
                <th>Alert</th>
                <th>Description</th>
              </tr>
            </thead>
            <tbody>
              {{#each model as |warning|}}
                <tr>
                  <td class={{warning.alertlevel}}>
                    <i class="fa fa-exclamation-triangle fa-2x" aria-hidden="true"></i>
                  </td>
                  <td>{{warning.description}}</td>
                </tr>
              {{/each}}
            </tbody>
          </table>
      </div>
    </div>
  </div>
</div>
